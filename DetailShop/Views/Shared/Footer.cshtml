
@{
    var listMenu = new DetailShop.Models.Menu.ListMenu();
}
<footer>
    <div>
        <nav>
            <ul>
                @foreach (var item in listMenu.FooterMenu)
                {
                    <li class="footerli"><a class="aClass" href="@Url.Action(item.Action, item.Controller)">@item.Label</a></li>
                }
            </ul>
        </nav>
    </div>
</footer>

