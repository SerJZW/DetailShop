@model List<DetailShop.Models.DbModels.Discounts>
<h1>На сайте присутствуют скидки</h1>
@foreach (var discounts in @Model)
{
    <div class="divBlock">
        <div class="wrapper">
            <div class="content">
                <p>При покупках на сумму: @discounts.Total</p>
                <p>Скидка: @discounts.Discount %</p>
            </div>
        </div>
    </div>
}

